{
  "name": "eslint-format",
  "main": "./lib/eslint-format",
  "version": "1.0.20",
  "description": "Format code with Atom IDE and ESLint",
  "keywords": [
    "format",
    "eslint"
  ],
  "repository": "https://github.com/AtomFormat/eslint-format",
  "license": "MIT",
  "engines": {
    "atom": ">=1.24.0 <2.0.0"
  },
  "scripts": {
    "test": "atom --test spec",
    "lint": "eslint ."
  },
  "atomTestRunner": "atom-jasmine3-test-runner",
  "providedServices": {
    "code-format.range": {
      "versions": {
        "0.1.0": "provideRangeCodeFormat"
      }
    },
    "code-format.onSave": {
      "versions": {
        "0.1.0": "provideOnSaveCodeFormat"
      }
    }
  },
  "dependencies": {
    "eslint": "^7.18.0"
  },
  "devDependencies": {
    "@semantic-release/apm-config": "^8.0.0",
    "atom-jasmine3-test-runner": "^5.1.8",
    "semantic-release": "^17.3.3"
  },
  "release": {
    "extends": "@semantic-release/apm-config"
  }
}
