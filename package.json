{
  "name": "eslint-format",
  "main": "./lib/eslint-format",
  "version": "0.4.0",
  "description": "Format code with Atom IDE and ESLint",
  "keywords": [
    "format",
    "eslint"
  ],
  "repository": "https://github.com/AtomFormat/eslint-format",
  "license": "MIT",
  "engines": {
    "atom": ">=1.24.0 <2.0.0"
  },
  "scripts": {
    "test": "atom --test spec",
    "lint": "eslint .",
    "semantic-release": "semantic-release"
  },
  "atomTestRunner": "atom-jasmine3-test-runner",
  "providedServices": {
    "code-format.range": {
      "versions": {
        "0.1.0": "provideRangeCodeFormat"
      }
    },
    "code-format.onSave": {
      "versions": {
        "0.1.0": "provideOnSaveCodeFormat"
      }
    }
  },
  "dependencies": {
    "eslint": "^5.16.0"
  },
  "devDependencies": {
    "@semantic-release/apm": "^1.0.2",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/git": "^7.0.9",
    "@semantic-release/github": "^5.3.0",
    "@semantic-release/release-notes-generator": "^7.1.6",
    "atom-jasmine3-test-runner": "^4.1.2",
    "semantic-release": "15.13.8"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/github",
      "@semantic-release/apm",
      "@semantic-release/git"
    ]
  }
}
