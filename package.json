{
  "name": "eslint-format",
  "main": "./lib/eslint-format",
  "version": "0.4.9",
  "description": "Format code with Atom IDE and ESLint",
  "keywords": [
    "format",
    "eslint"
  ],
  "repository": "https://github.com/AtomFormat/eslint-format",
  "license": "MIT",
  "engines": {
    "atom": ">=1.24.0 <2.0.0"
  },
  "scripts": {
    "test": "atom --test spec",
    "lint": "eslint .",
    "semantic-release": "semantic-release"
  },
  "atomTestRunner": "atom-jasmine3-test-runner",
  "providedServices": {
    "code-format.range": {
      "versions": {
        "0.1.0": "provideRangeCodeFormat"
      }
    },
    "code-format.onSave": {
      "versions": {
        "0.1.0": "provideOnSaveCodeFormat"
      }
    }
  },
  "dependencies": {
    "eslint": "^6.5.1"
  },
  "devDependencies": {
    "@semantic-release/apm-config": "^6.0.2",
    "atom-jasmine3-test-runner": "^4.3.9",
    "semantic-release": "^15.13.29"
  },
  "release": {
    "extends": "@semantic-release/apm-config"
  }
}
